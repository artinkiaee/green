<%- include('partials/navs', { title: 'g' }) %>
<section class="hero-section d-flex flex-wrap">
    <div class="video-container1">
        <video autoplay muted loop>
            <source src="/<%= pro.thumbnail.url %>" type="video/mp4">
        </video>
    </div>
    <div class="content-container">
        <div class="content-wrap">
            <h1 class="display-4 mb-4 text-success"><%= pro.title %></h1>
            <p class="lead mb-5"><%= pro.description %></p>
            <div class="row">
                <!-- Feature Card 1 -->
                <div class="col-md-6 mb-4">
                    <div class="feature-card">
                        <div class="feature-icon">
                            <i class="fa-solid fa-headset"></i>
                        </div>
                        <div class="feature-content">
                            <h3 class="text-success">مشاوره تخصصی</h3>
                            <p>مشاوره تخصصی: 02141749</p>
                        </div>
                    </div>
                </div>
                <!-- Feature Card 2 -->
                <div class="col-md-6 mb-4">
                    <div class="feature-card">
                        <div class="feature-icon">
                            <i class="fa-solid fa-location-dot"></i>
                        </div>
                        <div class="feature-content">
                            <h3 class="text-success">نمایندگان فعال</h3>
                            <p>نمایندگی در سراسر ایران</p>
                        </div>
                    </div>
                </div>
                <!-- Feature Card 3 -->
                <div class="col-md-6 mb-4">
                    <div class="feature-card">
                        <div class="feature-icon">
                            <i class="fa-solid fa-tools"></i>
                        </div>
                        <div class="feature-content">
                            <h3 class="text-success">گارانتی و پشتیبانی</h3>
                            <p>گارانتی 5 ساله کمپرسور</p>
                        </div>
                    </div>
                </div>
                <!-- Feature Card 4 -->
                <div class="col-md-6 mb-4">
                    <div class="feature-card">
                        <div class="feature-icon">
                            <i class="fa-solid fa-shield-halved"></i>
                        </div>
                        <div class="feature-content">
                            <h3 class="text-success">گارانتی قطعات</h3>
                            <p>گارانتی 18 ماه سایر قطعات</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<section style="width: 100%;padding: 60px;">
    <div class="d-flex justify-content-center align-items-center align-content-center">
        <hr style="width: 40%;height: 3px;background: var(--bs-success);">
        <h1 class="text-center" style="color: var(--bs-success);margin-right: 12px;margin-left: 12px;font-size: 35px;">سری محصول</h1>
        <hr style="width: 40%;height: 3px;background: var(--bs-success);">
    </div>
    <div class="container" style="padding: 40px;">
        <div class="row" style="margin-bottom: 50px;">
            <% sub.forEach(function(item, index) { %>
                <% if (index % 3 === 0 && index !== 0) { %>
                </div><div class="row" style="margin-bottom: 50px;">
                <% } %>
            <div class="col-md-4">
                <div class="card text-center d-flex justify-content-center align-items-center align-content-center" style="border-radius: 0px;">
                    <div class="card-body d-flex flex-column justify-content-center align-items-center align-content-center" style="padding: 0;"><img style="padding:20px;width: 100%;height:300px; object-fit: contain;" src="/<%= item.thumbnail %>">
                        <div style="width: 100%;padding: 20px;border-top: 1px solid rgb(210,210,210) ;"><a class="pro" href="#"><%= item.title %></a></div>
                    </div>
                </div>
            </div>
            <% }); %>
        </div>
    </div>
</section>
<section style="width: 100%;padding: 60px;">
    <div class="d-flex justify-content-center align-items-center align-content-center">
        <hr style="width: 40%;height: 3px;background: var(--bs-success);">
        <h1 class="text-center" style="color: var(--bs-success);margin-right: 12px;margin-left: 12px;font-size: 35px;">دانلود کاتالوگ</h1>
        <hr style="width: 40%;height: 3px;background: var(--bs-success);">
    </div><button class="btn btn-success btn-lg d-flex justify-content-center align-items-center align-content-center mx-auto" type="button" style="margin-top: 30px;"><svg xmlns="http://www.w3.org/2000/svg" enable-background="new 0 0 24 24" height="1em" viewBox="0 0 24 24" width="1em" fill="currentColor" class="fs-4" style="margin-left: 5px;">
